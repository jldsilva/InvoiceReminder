{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Timeout": 120,
  "ProviderName": "Npgsql",
  "appKeys": {
    "googleOauthClientId": "SECRET_CLIENT_ID",
    "googleOauthClientSecret": "SECRET_CLIENT_SECRET",
    "googleOauthRedirectUri": "REDIRECT_URI",
    "telegramBotToken": "SECRET_BOT_TOKEN",
    "tokenEncryptionKey": "SECRET_TOKEN_ENCRYPTION_KEY"
  },
  "ConnectionStrings": {
    "DatabaseConnection": "SECRET_DB_CONNECTION"
  },
  "JwtOptions": {
    "Issuer": "SECRET_ISSUER",
    "Audience": "SECRET_AUDIENCE",
    "SecretKey": "SECRET_KEY"
  }
}
